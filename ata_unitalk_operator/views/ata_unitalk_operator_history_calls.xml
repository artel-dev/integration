<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="tmp_data_history_calls" name="Data history calls">
        <t>
            <div class="oe_structure">
                <div class="container"><br />
                    <center>
                        <h3>Data Calls</h3>
                    </center><br />
                    <table class="table-striped table">
                        <thead style="font-size: 23px;">
                            <tr>
                                <h6><b>
                                    <th>outer Number</th>
                                    <th>id</th>
                                    <th>direction</th>
                                    <th>date</th>
                                </b></h6>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="[1, 2, 3]" t-as="i">
                                <tr>
                                    <td><span t-out="i"/></td>
                                    <td><span t-out="i"/></td>
                                    <td><span t-out="i"/></td>
                                    <td><span t-out="i"/></td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </div>
            </div>
        </t>
    </template>
</odoo>