<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="ata_unitalk_operator_view_form" model="ir.ui.view">
        <field name="name">Unitalk Operator Form</field>
        <field name="model">ata.unitalk.operator</field>
        <field name="arch" type="xml">
            <form>
                <header>
                    <button string="Start Call" type="object" name="start_call" class="oe_highlight"/>
                     <button string="Calls History" type="object" name="get_history" class="oe_highlight"/>
                </header>
                <group>
                    <field name="phone_number"/>
                </group>
            </form>
        </field>
    </record>

    <record id="ata_unitalk_operator_action" model="ir.actions.act_window">
        <field name="name">Unitalk Operator</field>
        <field name="res_model">ata.unitalk.operator</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

</odoo>