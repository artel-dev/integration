<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">
    <record id="ata_exchange_objects_cron" model="ir.cron">
        <field name="name">ATA : Exchange of objects with external systems</field>
        <field name="model_id" ref="model_ata_exchange_objects"/>
        <field name="state">code</field>
        <field name="code">model.exchange()</field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>

    <record id="ata_exchange_objects_cron_immediately" model="ir.cron">
        <field name="name">ATA : Exchange of objects with external systems (immediately)</field>
        <field name="model_id" ref="model_ata_exchange_objects"/>
        <field name="state">code</field>
        <field name="code">model.exchange_immediately()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">1</field>
        <field name="active">True</field>
    </record>
</odoo>