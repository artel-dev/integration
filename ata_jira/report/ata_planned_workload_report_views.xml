<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="ata_jira_planned_workload_report_paperformat" model="report.paperformat">
        <field name="name">Integration report (A4)</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="orientation">Portrait</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="margin_top">5</field>
        <field name="margin_bottom">5</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">0</field>
    </record>

    <record id="ata_jira_planned_workload_report_report" model="ir.actions.report">
        <field name="name">Planned workload report</field>
        <field name="model">project.task</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">ata_jira.ata_jira_planned_workload_report_template</field>
        <field name="report_file">ata_jira.ata_jira_planned_workload_report_template</field>
        <field name="print_report_name">('Planned workload report - %s' % (object.name))</field>
        <field name="binding_model_id" ref="model_project_task"/>
        <field name="binding_type">report</field>
        <field name="paperformat_id" ref="ata_jira.ata_jira_planned_workload_report_paperformat"/>
    </record>

</odoo>
